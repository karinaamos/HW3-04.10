#include "stdio.h"

int main() {
    int x,y;
    scanf("%d %d", &x, &y);
    if (x>0 && y>0){
        printf("1 четверть");
    }
    if (x>0 && y<0){
        printf("4 четверть");
    }
    if (x<0 && y>0){
        printf("2 четверть");
    }
    if (x<0 && y<0){
        printf("3 четверть");
    }
    if (x == 0 && (y>0 || y<0)){
        printf("Ось Oy");
    }
    if (y==0 && (x>0 || x<0)){
        printf("Ось Ox");
    }
    if (x==0 && y==0){
        printf("Начало координат");
    }
    return 0;
}
